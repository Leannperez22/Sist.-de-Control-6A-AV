    <script>
        const $container = document.getElementById("container")
        const num = Number(prompt("Dame un numero"))
        let results = []

        for (let i = 0; i <= 9; i++) {
            results.push(num * i)
        }

       document.write('<p><a href="../index.html">LocalHost</a></p>')
        })
    </script>
